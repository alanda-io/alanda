    "publish-stylelint": "cd projects/alanda-stylelint-config-standard && npm login --registry=https://repo.alanda.io/repository/alanda/ && npm publish",
    "watch": "ng --version && ng build alanda-common --watch"
  },
  "private": true,
  "dependencies": {
