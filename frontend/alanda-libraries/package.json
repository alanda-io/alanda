    "@angular/core": "~9.1.2",
    "@angular/forms": "~9.1.2",
    "@angular/platform-browser": "~9.1.2",
    "@angular/platform-browser-dynamic": "~9.1.2",
    "@angular/router": "~9.1.2",
    "@rx-angular/state": "~1.1.1",
    "bpmn-js": "6.5.1",
    "font-awesome": "4.7.0",
    "primeflex": "1.0.0",
    "primeicons": "2.0.0",
