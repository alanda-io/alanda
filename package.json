{
  "name": "alanda",
  "version": "0.0.1",
  "description": "A framework for developing processes the easy way.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cm": "npx git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanda-io/alanda.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/alanda-io/alanda/issues"
  },
  "homepage": "https://github.com/alanda-io/alanda#readme",
  "devDependencies": {
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "____prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
